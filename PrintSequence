using System;

namespace PrintSequence
{
    class PrintSequence
    {
        static void Main()
        {

            for (int i = 0; 2 < 12; i++)
            {
                if (i % 2 == 0)
                {
                    Console.Write(i + " ");
                }
                else
                {
                    Console.Write(-i + " ");
                }
            }
        } 
    }
}

